---

import RegImage from "./RegImage.astro"

const { Img01, Img02, Img03 } = Astro.props

---

<div class="container">
    <div class="single">
        <RegImage src={Img01} alt="" variant="default" />
    </div>
    <div class="single">
        <RegImage src={Img02} alt="" variant="default" />
    </div>
    <div class="single">
        <RegImage src={Img03} alt="" variant="default" />
    </div>
    </div>

    



<style>

    .container {
        display: flex;
        flex-direction: column;
        gap: var(--x-small);
        padding: var(--none) var(--xx-small);
        width: 183px;
        background-color: var(--white-smpke);
    }

    .single {
        aspect-ratio: 1 / 1.25;
        overflow: hidden;
    }

</style>